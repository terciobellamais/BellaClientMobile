{
  "expo": {
    "name": "Bella +",
    "slug": "BellaClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "com.bellaclient.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#FF8373",
      "imageWidth": 10
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "@react-native-google-signin/google-signin"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bellaclient.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#FF8373"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.bellaclient.app",
            "CFBundleURLSchemes": ["com.bellaclient.app"]
          }
        ]
      }
    },
    "android": {
      "compileSdkVersion": 33,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#FF8373"
      },
      "package": "com.googleauth.bella",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-3940256099942544~3347511713"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9d7e8586-b9cf-4805-ab58-6fb8dd598356"
      }
    }
  }
}
